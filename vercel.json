{
    "version": 2,
    "builds": [
      {
        "src": "src/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "src/index.js"
      }
    ],
    "rewrites": [
      {
        "source": "/api-docs/(.*)",
        "destination": "/api-docs/index.html"
      }
    ]
  }